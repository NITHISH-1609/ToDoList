<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= wday %> </h1>
    </div>
    <div class="box">
        <%for(var i=0;i<Addme.length;i++){%>
            <div class="item">
                <input type="checkbox">
                <p><%= Addme[i] %> </p>

            </div>
        <%}%>

        <form class="item" action="/" method="POST">
            <input type="text" id="text" name="text" placeholder="New Item" autocomplete="off">
            <button type="submit" name="button" value=<%=wday %>>+</button>
          </form>
    </div>


    <%- include("footer") -%>
   
   
    